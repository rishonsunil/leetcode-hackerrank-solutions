class Solution(object):
    def generate(self, numRows):
        """
        :type numRows: int
        :rtype: List[List[int]]
        """
        triangle = []
        for rows in range(numRows):
            if rows == 0:
                triangle.append([1])
            else:
                prev = triangle[-1]
                new = [1]
                for i in range(1,rows):
                    new.append(prev[i-1]+prev[i])
                new.append(1)
                triangle.append(new)
        return triangle
