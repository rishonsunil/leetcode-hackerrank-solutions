class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        strnum = ""
        result = []
        for i in range(len(digits)):
            strnum = strnum + str(digits[i])
        num = int(strnum)
        num = num + 1
        for i in str(num):
            result.append(int(i))
        return result
sol = Solution()
print(sol.plusOne([1,2,3]))
