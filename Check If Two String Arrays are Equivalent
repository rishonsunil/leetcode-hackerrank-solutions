class Solution(object):
    def arrayStringsAreEqual(self, word1, word2):
        """
        :type word1: List[str]
        :type word2: List[str]
        :rtype: bool
        """
        words1=""
        words2=""
        for x in word1:
            words1+=x
        for y in word2:
            words2+=y
        if words1==words2:
            return True
        else:
            return False
