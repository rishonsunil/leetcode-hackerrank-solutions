class Solution(object):
    def getRow(self, rowIndex):
        """
        :type rowIndex: int
        :rtype: List[int]
        """
        triangle = []
        for rows in range(rowIndex+1):
            if rows == 0:
                triangle.append([1])
            else:
                prev = triangle[-1]
                node = [1]
                for i in range(1,rows):
                    node.append(prev[i-1]+prev[i])
                node.append(1)
                triangle.append(node)
        return triangle[rowIndex]
