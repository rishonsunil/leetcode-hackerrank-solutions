class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        res = []
        for i in nums:
            if i in res:
                pass
            else:
                res.append(i)
        for i in range(len(res)):
            nums[i]=res[i]
        return len(res)
sol = Solution()
arr = [1,1,2]
length= sol.removeDuplicates(arr)
print(length)
print(arr[:length])
