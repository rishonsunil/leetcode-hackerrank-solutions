class Solution(object):
    def alternatingSum(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        one=0
        two=0
        for x in range(0,len(nums),2):
            one+=nums[x]
        for y in range(1,len(nums),2):
            two+=nums[y]
        return one-two
